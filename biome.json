{
  "$schema": "node_modules/@biomejs/biome/configuration_schema.json",
  "organizeImports": {
    "enabled": true
  },
  "vcs": {
    "enabled": true,
    "clientKind": "git"
  },
  "javascript": {
    "formatter": {
      "trailingCommas": "es5"
    }
  },
  "formatter": {
    "enabled": true,
    "indentStyle": "space"
  },
  "linter": {
    "enabled": true
  },
  "files": {
    "ignore": [
      "node_modules"
    ]
  }
}